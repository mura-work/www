<h2>GS一覧</h2>

<div class="col-xs-offset-1 col-xs-10">

		<% @users.each do |user| %>
			<%= link_to user_path(user) do %>
		    <div class="users-index">
		      <%= attachment_image_tag user, :image,  :fill, 70, 70, :class => 'img-circle', fallback: "no_image.jpg" %>
		      <div class="users-index-boxes">
		        <div class="users-index-name"><%= user.name %></div>
		        <div class="users-index-box"><%= user.format %></div>
		      </div>
		    </div>
		    <% end %>
		<% end %>
</div>


