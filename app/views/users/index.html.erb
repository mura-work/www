<h2>GS一覧</h2>

<div class="col-xs-offset-1 col-xs-10">
<table class="table table-striped">
	<thead>
		<tr>
			<th></th>
			<th></th>
			<th></th>
		</tr>
	</thead>

	<tbody>
		<% @users.each do |user| %>
		    <div class="books-index-item">
		      <tr>
		        <td><%= attachment_image_tag user, :image, :fill, 60, 60, fallback: "no_image.jpg" %></td>
		        <td><%= link_to user.name, user_path(user) %><br><br></td>
		        <td><%= link_to 'Show', user_path(user) %></td>
		      </tr>
		    </div>
		<% end %>
	</tbody>

</table>
</div>


